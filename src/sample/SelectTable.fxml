<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<StackPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
  prefHeight="125.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8.0.141"
  xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.SelectTableController">
  <children>
    <MenuButton fx:id="menuButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false"
      text="AUTHORS" StackPane.alignment="CENTER">
      <items>
        <MenuItem mnemonicParsing="false" onAction="#AuthorsMenuItemClicked" text="AUTHORS"/>
        <MenuItem mnemonicParsing="false" onAction="#TitlesMenuItemClicked" text="TITLES"/>
      </items>
    </MenuButton>
    <Button mnemonicParsing="false" onAction="#onShowTableButtonClicked" text="Show Table"
      StackPane.alignment="BOTTOM_RIGHT"/>
    <Label text="Select Table:" StackPane.alignment="TOP_LEFT">
      <font>
        <Font name="System Bold" size="16.0"/>
      </font>
    </Label>
  </children>
  <padding>
    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
  </padding>
</StackPane>
